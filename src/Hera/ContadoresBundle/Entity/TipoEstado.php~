<?php

namespace Hera\ContadoresBundle\Entity;

/**
 * TipoEstado
 */
class TipoEstado
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nombre;

    /**
     * @var boolean
     */
    private $esEstadoFinal = '0';


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nombre
     *
     * @param string $nombre
     *
     * @return TipoEstado
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;

        return $this;
    }

    /**
     * Get nombre
     *
     * @return string
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Set esEstadoFinal
     *
     * @param boolean $esEstadoFinal
     *
     * @return TipoEstado
     */
    public function setEsEstadoFinal($esEstadoFinal)
    {
        $this->esEstadoFinal = $esEstadoFinal;

        return $this;
    }

    /**
     * Get esEstadoFinal
     *
     * @return boolean
     */
    public function getEsEstadoFinal()
    {
        return $this->esEstadoFinal;
    }
}

